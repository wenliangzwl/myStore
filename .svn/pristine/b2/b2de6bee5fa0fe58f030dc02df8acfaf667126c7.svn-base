}

.footer_b .tb6 .f_title {
	background-position: 0px -200px;
}

.footer_m {
	border-top: 1px solid #ebebeb;
	width: 100%;
	padding: 17px 0 50px;
	color: #8c8c8c;
	font-size: 12px;
	text-decoration: none;
}

.footer_nav {
	text-align: center;
	line-height: 20px;
	padding-bottom: 17px;
}

.footer_nav a {
	color: #666666;
	padding: 0 14px;
}

.footer_copyright {
	color: #666666;
	text-align: center;
	width: 100%;
}




