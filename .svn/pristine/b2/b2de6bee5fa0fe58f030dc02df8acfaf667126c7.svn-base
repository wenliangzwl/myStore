@charset "utf-8";

/* CSS Document */



body {
	font-size: 12px;
	font-family: "微软雅黑";
	color: #5e5e5e;
	background: #fff;
	overflow-x: hidden;
	-webkit-backface-visibility: hidden;
}

/*右侧悬浮导航*/
.float{width:111px;height:224px;position:fixed;_position:absolute;display:none;overflow:hidden;right:0%; z-index:50}/*left50%确保页面中间，左边距515大于页面内容的一半，确保在内容整体右侧外*/
.float a{display:block;height:22px; width:80px;padding-left:10px; font-size:medium;margin-top: 5px;padding-top: 7px;}

ul,li,form,input,a,span,div,h1,h2,h3,h4,h5,h6,body,table,textarea,p,dl,dt,dd
	{
	margin: 0;
	padding: 0;
}

ul,li {
	list-style: none;
}

table {
	cellspacing: 0px;
	cellpadding: 0px;
	border-spacing: 0px;
}

img {
	border: none;
	vertical-align: middle;
}

input {
	font-family: "微软雅黑";
	border: none;
}

input:focus {
	outline: none;
	background-image: none;
	border-color: #ccc;
}

input[type=button] {
	cursor: pointer;
}

textarea {
	resize: none;
	outline: none;
	font-family: monospace;
}

a {
	text-decoration: none;
	color: #393737;
}

a:hover {
	color: #F00;
}

/**解决浮动后div不自动撑开**/
div:after,ul:after,li:after {
	content: "";
	display: block;
	clear: both;
	height: 0px;
	visibility: hidden;
}

div,ul,li { *
	display: inline-block;
	zoom: 1;
}

.clearfloat {
	font-size: 0px;
	line-height: 0px;
	float: none;
	clear: both;
	display: block;
	height: 0px; +
	height: auto;
}

.red {
	color: #F00;
	font-family: "NSimSun";
}



.footer {
	width: 100%;
	margin-top: 30px;
	border-top: 6px solid #ee0a15;
}

.footer .footer_t {
	background-color: #fafafa;
	border-bottom: 1px solid #ebebeb;
	width: 100%;
	text-align: center;
}

.footer .footer_t ul {
	width: 908px;
	margin: 0 auto;
	padding: 7px 0;
}

.footer .footer_t ul li {
	width: 130px;
	height: 52px;
	float: left;
	padding: 0 48px;
	display: inline;
	border-right: 1px solid #dedede;
